{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "outDir": "dist",  // <- Ensure this is set
    "rootDir": "src",  // <- Ensure this is set
    "strict": true,
    "esModuleInterop": true
  },
  "include": ["src/**/*"],  // <- Ensures all files in src are compiled
  "exclude": ["node_modules"]
}
